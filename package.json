{
  "name": "bda-engine",
  "version": "0.0.0",
  "description": "A simple game framework built in TS",
  "main": "./build/exports.js",
  "types": "./typings/types.d.ts",
  "files": [
    "build/src",
    "typings"
  ],
  "scripts": {
    "exports": "node ./scripts/exports.mjs",
    "typings": "node ./scripts/typings.mjs",
    "build-engine": "webpack",
    "build-transformer": "cd builder && webpack",
    "build": "npm run exports && npm run typings && npm run build-transformer && npm run build-engine"
  },
  "exports": {
    ".": {
      "default": "./dist/lib.js",
      "engine": "./dist/lib.js",
      "transformer": "./dis/transformer.js"
    }
  },
  "keywords": [
    "Game",
    "Engine",
    "Typescript",
    "ECS"
  ],
  "author": "Bagel03",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bagel03/Doge-Engine/issues"
  },
  "homepage": "https://github.com/Bagel03/Doge-Engine#readme",
  "dependencies": {
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "ts-loader": "^9.3.0",
    "webpack-cli": "^4.10.0"
  }
}
